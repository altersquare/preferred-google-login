{
	"manifest_version": 3,
	"name": "Default Google User",
	"version": "5.0",
	"description": "Adds the authuser parameter to domain.",
	"permissions": [
		"scripting",
		"storage"
	],
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"action": {
		"default_popup": "src/popup.html"
	},
	"host_permissions": [
		"*://*/*"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"src/content.js"
			],
			"run_at": "document_start"
		}
	]
}
