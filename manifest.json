{
	"manifest_version": 3,
	"name": "Default Google User",
	"version": "2.0",
	"description": "Adds the authuser parameter to domains (toggleable).",
	"permissions": [
		"scripting",
		"storage"
	],
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.youtube.com/*",
				"*://*.gemini.google.com/*"
			],
			"js": [
				"content.js"
			],
			"run_at": "document_start"
		}
	]
}
