{
	"manifest_version": 3,
	"name": "Default Google User",
	"version": "4.0",
	"description": "Adds the authuser parameter to domain.",
	"permissions": [
		"scripting",
		"storage"
	],
	"action": {
		"default_popup": "popup.html"
	},
	"host_permissions": [
		"*://*/*"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content.js"
			],
			"run_at": "document_start"
		}
	]
}
